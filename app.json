{
    "expo": {
        "name": "SmartApp",
        "slug": "SmartApp",
        "description": "React Native - Soft UI Kit - Free",
        "primaryColor": "#CB0C9F",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#a4c9ec"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#a4c9ec"
            },
            "package": "com.neymarjohn.SmartApp"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}